/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun4i-a10";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			serial5 = "/soc/serial@1c29400";
		};
	};

	fragment@1 {
		target = <&pio>;
		__overlay__ {
			uart5_pins: uart5-pins {
				pins = "PH6", "PH7";
				function = "uart5";
			};
		};
	};

	fragment@2 {
		target = <&uart5>;
		 __overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&uart5_pins>;
			status = "okay";
		};
	};
};
