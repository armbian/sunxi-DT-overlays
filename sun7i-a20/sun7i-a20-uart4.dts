/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun7i-a20";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			serial4 = "/soc@1c00000/serial@1c29000";
		};
	};

	fragment@1 {
		target = <&uart4>;
		 __overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&uart4_pins_a>;
			status = "okay";
		};
	};
};
